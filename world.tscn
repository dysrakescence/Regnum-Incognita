var active := false
var time := 0.0

func _ready():
	pass
	#rotation_degrees.x = 270

func _process(delta):
	rotation_degrees.x += delta / 12
	time += delta
	if time >= 2160:
		time -= 2160
		active = !active
	if active:
		light_energy = lerp(light_energy, 0.1, delta/36)
	else:
		light_energy = lerp(light_energy, 0.0, delta/36)
"

